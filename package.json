{
  "name": "eco-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy:stop-container": "docker compose stop || true",
    "deploy:remove-container": "docker compose down || true",
    "deploy:start-container": "docker compose up --build -d",
    "deploy": "npm run deploy:stop-container && npm run deploy:remove-container && npm run deploy:start-container"
  },
  "dependencies": {
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "typescript": "^5"
  }
}
